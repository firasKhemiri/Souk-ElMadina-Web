                        cats =undefined;


                    //   alert("hheyy art"+nom_art+" b "+nom_bout+" pmin "+pmin+" pmax "+pmax+"cats "+cats);

                    getAllArticles(1);

                });

                $("#tri_type").change(function () {
                    tri_type = $("#tri_type").val();

                 //   alert(tri_type);
                    getAllArticles(1);

                })


            });







        </script>


