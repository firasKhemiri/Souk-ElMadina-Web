        <script type="text/javascript">


            var first = true;

            var nom_art;
            var cats;
            var nom_bout;
            var pmin;
            var pmax;
            var gov;
            var tri_type;

            var sliderValue;

            function getAllArticles(page) {

                var jsonString = JSON.stringify(cats);

                //  alert("jso"+jsonString);

                $.ajax({
                    url: "{{ path('filterArts') }}",
                    type: "POST",
                    data: {page:page,
                        nom_art:nom_art,
                        cats:jsonString,
                        nom_bout:nom_bout,
                        type_tri:tri_type,
                        pmin:pmin,
                        pmax:pmax,
                        gov:gov},

                    beforeSend: function () {
                        //    alert(page);
                        //  document.getElementById("pagination").innerHTML = "";
                        i=0;

                        $('#pagination').fadeOut();
                        $('#prodsec').fadeOut();
                    },
                    complete: function () {

                    },
                    success: function (data) {
                        var response = jQuery.parseJSON(data);
                        console.log(response);

                        if (response.nbrc===0)
                        {
                            document.getElementById("prodsec").innerHTML = "";

                            $html="<h1> Aucun élément trouvé";
                            document.getElementById("prodsec").innerHTML += $html;

                            $('#prodsec').fadeIn();

                        }

                        else
                        {

                            document.getElementById("prodsec").innerHTML = "";


                            if (first) {
                                init(response.pages);
                                first= false;
                            }

                            for (i=0;i<response.nbrc;i++) {

                                if (response[i].oldprix === "" && !response[i].new) {
                                    $html =
                                        "<div class=\" col-sm-12 col-md-6 col-lg-4 p-b-50\">\n" +
                                        "                        <!-- Block2 -->\n" +
                                        "                        <div class=\"block2\">\n" +
                                        "                            <div class=\"block2-img wrap-pic-w of-hidden pos-relative block2-labelnew\">";
                                }

                                else if (response[i].oldprix <= response[i].prix && !response[i].new) {
                                    $html =
                                        "<div class=\"col-sm-12 col-md-6 col-lg-4 p-b-50\">\n" +
                                        "                        <!-- Block2 -->\n" +
                                        "                        <div class=\"block2\">\n" +
                                        "                            <div class=\"block2-img wrap-pic-w of-hidden pos-relative block2-labelnew\">";
                                }


                                else if (response[i].oldprix > response[i].prix) {
                                    $html =
                                        "<div class=\"col-sm-12 col-md-6 col-lg-4 p-b-50\">\n" +
                                        "                        <!-- Block2 -->\n" +
                                        "                        <div class=\"block2\">\n" +
                                        "                            <div class=\"block2-img wrap-pic-w of-hidden pos-relative block2-labelsale\">";
                                }


                                $html += "                                " +
                                    "<img id='pimg" + i + "' class =\"IMG-ART \" src=\"" + response[i].img + " \" alt=\"IMG-PRODUCT\">\n"  +


                                    "                                <div class=\"block2-overlay trans-0-4\" >\n" +

                                     "<a href=\"article/"+response[i].id+"\" >\n" +
                                "                                        <i class=\"block2-overlay trans-1-0\"></i>\n" +
                                "                                    </a>\n" +

                                    "                                    <a href=\"#\" class=\"block2-btn-addwishlist hov-pointer trans-0-4\">\n" +
                                    "                                        <i class=\"icon-wishlist icon_heart_alt\" aria-hidden=\"true\"></i>\n" +
                                    "                                        <i class=\"icon-wishlist icon_heart dis-none\" aria-hidden=\"true\"></i>\n" +
                                    "                                    </a>\n" +
                                    "\n" +
                     //               " <div class=\"block2-overlay\" onclick=\"location.href='article/"+response[i].id+"';\" > </div> "+

                                    "                                    <div class=\"block2-btn-addcart w-size2 trans-0-4\">\n" +
                                    "                                        <!-- Button -->\n" +
                                    "                                        <button class=\"flex-c-m size2 bg4 bo-rad-23 hov1 s-text1 trans-0-4\">\n" +
                                    "                                            Ajouter au panier\n" +
                                    "                                        </button>\n" +
                                    "                                    </div>\n" +


                                    "                                </div>\n" +
                                    "                            </div>";


                                if (response[i].oldprix === "") {
                                    $html +=
                                        "         <div class=\"block2-txt p-t-20\">" +
                                        "              <a href=\"product-detail.html\" class=\"block2-name dis-block s-text3 p-b-5\">" +
                                        "                    " + response[i].name +
                                        "              </a>" +

                                        "              <span class=\"block2-price m-text6 p-r-5\">" +
                                        "                    " + response[i].prix +
                                        "              </span>" +
                                        "         </div>" +
                                        "    </div>" +
                                        "</div>";
                                }

                                else {
                                    $html +=
                                        "         <div class=\"block2-txt p-t-20\">" +
                                        "              <a href=\"product-detail.html\" class=\"block2-name dis-block s-text3 p-b-5\">" +
                                        "                    " + response[i].name +
                                        "              </a>" +

                                        "              <span class=\"block2-oldprice m-text7 p-r-5\">" +
                                        "                   " + response[i].oldprix +
                                        "              </span>" +

                                        "              <span class=\"block2-newprice m-text8 p-r-5\">" +
                                        "                    " + response[i].prix +
                                        "              </span>" +
                                        "         </div>" +
                                        "    </div>" +
                                        "</div>";
                                }


                                document.getElementById("prodsec").innerHTML += $html;


                                $('#prodsec').fadeIn();


                                var im = $(".block2").hide();
                            }

                        }


                        $(".IMG-ART").each(function(i) {
                            $(this).on('load', function() {
                                $(im).fadeIn(1000);

                            });
                        });


                        $('#pagination').fadeIn();

                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                        alert(errorThrown)
                    }
                });

            }




            function getCat() {


                $.ajax({
                    url: "{{ path('getCats') }}",
                    type: "GET",
                    data: {},

                    beforeSend: function () {
                    },
                    complete: function () {

                    },
                    success: function (data) {
                        var response = jQuery.parseJSON(data);
                        console.log(response);

                        for (i=0;i<response.nbrc;i++) {

                            document.getElementById("cats").innerHTML +=
                                "<option value='"+response[i].name+"'>"+response[i].name+"</option>"
                        }
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                        alert(errorThrown)
                    }
                });

            }



            $(document).ready(function() {
                first = true;

                nom_art="";
                nom_bout="";
                cat="";
                gov="";
                pmin=0;
                pmax=1500;
                tri_type='';
                getAllArticles(1);

